function Dog(ownersName, dogsName, color) {
    this.owner,sName = ownersName;
    this.dogsName = dogsName;
    this.color = color;
    this.numLegs = 4;
}


let dog1 = new Dog("Bob", "Fluffy", "Brown");
let dog2 = new Dog("Michael", "Chloe", "Tan");

console.log(Dog1="Bob has a", Dog1="Brown dog named", Dog1="Fluffy");
console.log(Dog2="Michael has a", Dog2="Tan dog named", dog2="Chloe");


document.getElementById('Puppy').innerHTML = Dog.type;



